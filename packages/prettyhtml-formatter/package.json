  "files": [
    "index.js",
    "stringify.js"
  ],
  "keywords": [
    "formatter",
    "web-components",
    "html"
  ],
  "author": "Dustin Deus <deusdustin@gmail.com> (http://www.starptech.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StarpTech/prettyhtml/issues"
  },
  "homepage": "https://github.com/StarpTech/prettyhtml#readme",
  "dependencies": {
    "@starptech/prettyhtml-hast-to-html": "^0.0.2",
    "hast-util-phrasing": "^1.0.1",
    "html-whitespace-sensitive-tag-names": "^1.0.0",
    "is-hidden": "^1.1.1",
    "rehype-minify-whitespace": "^2.0.3",
    "repeat-string": "^1.6.1",
    "to-vfile": "^5.0.0",
    "unified": "^7.0.0",
    "unist-util-visit-parents": "^1.1.2",
    "xtend": "^4.0.1",
    "@starptech/prettyhtml-rehype-parse": "^0.0.2"
  },
  "devDependencies": {
    "bail": "^1.0.1",
    "browserify": "^14.1.0",
    "esmangle": "^1.0.0",
    "is-hidden": "^1.0.1",
    "negate": "^1.0.0",
    "nyc": "^11.0.0",
    "rehype": "^5.0.0",
    "tape": "^4.0.0",
    "to-vfile": "^2.1.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
